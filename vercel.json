{
    "version": 2,
    "framework": "sveltekit",
    "builds": [
        {
            "src": "package.json",
            "use": "@sveltejs/adapter-vercel"
        }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install"
}